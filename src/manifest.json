{
    "name": "BUPT-Course-Killer",
    "version": "0.0.1",
    "description": "BUPT 抢课助手",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "images/icon48.png",
        "default_title": "BUPT-Course-Killer",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "unlimitedStorage"
    ],
    "web_accessible_resources": [
        "scripts/lhgdialog.min.js"
    ]
}
